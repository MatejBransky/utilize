    "lint": "eslint .",
    "types": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "history": "*",
    "react": "*"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@utilize/eslint-config": "^1.0.0",
    "@utilize/prettier-config": "^1.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.22.0",
    "history": "^5.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.1.2",
    "vite": "^2.1.0"
